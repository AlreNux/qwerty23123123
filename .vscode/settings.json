{
    "typescript.tsdk": ".yarn/sdks/typescript/lib",
    "search.exclude": {
        "**/.yarn": true,
        "**/.pnp.*": true
    },
    "eslint.nodePath": ".yarn/sdks",
    "prettier.prettierPath": ".yarn/sdks/prettier/index.js",
    "typescript.enablePromptUseWorkspaceTsdk": true,
    "createReactTSXComponent.useArrowFunctionComponent": true,
    "createReactTSXComponent.stylesFormat": "Styled Components",
    "createReactTSXComponent.fileExtension": "tsx",
    "createReactTSXComponent.useReactFC": true,
    "svg.preview.background": "black"
}
